{
  "name": "fastify-auth-skeleton",
  "version": "0.5",
  "description": "A skeleton project with typescript, typeorm & jwt authentication",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "npx rimraf build",
    "build": "tsc",
    "start": "fastify start -l info build/app.js",
    "dev": "npm run clean && npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"fastify start --ignore-watch=.ts$ -w -l info -P build/app.js\""
  },
  "keywords": [],
  "author": "SEK-Consulting",
  "license": "MIT",
  "dependencies": {
    "@fastify/autoload": "^5.6.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/jwt": "^6.5.0",
    "@fastify/rate-limit": "^7.6.0",
    "@fastify/sensible": "^5.2.0",
    "bcrypt": "^5.1.0",
    "fastify": "^4.10.2",
    "fastify-cli": "^5.7.0",
    "fastify-plugin": "^4.4.0",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.30.0",
    "eslint-plugin-import": "^2.26.0",
    "fastify-tsconfig": "^1.0.1",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
